        </ul>*@
        @*<ul class="ul">
            <li>@Html.LabelFor(model => model.oldPMWeek)</li>
            <li>@Html.TextBoxFor(model => model.oldPMWeek, new { style = "width: 300px;" })</li>
        </ul>*@
        <ul class="ul">
            <li>@Html.LabelFor(model => model.Closed)</li>
            <li>@Html.RadioButtonFor(model => model.Closed, "Y") Y</li>
            <li>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</li>
            <li>@Html.RadioButtonFor(model => model.Closed, "N") N</li>
        </ul>
        <ul class="ul">
            <li>@Html.LabelFor(model => model.ReceptionName)</li>
            <li>@Html.DisplayFor(model => model.ReceptionName)</li>
        </ul>
        <ul class="ul">
            <li>
                <input type="submit" value="確定" style="width: 80px" />&nbsp;&nbsp;&nbsp;</li>
