<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Themes/Shared/blackbird/main.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Themes/Shared/alert/main.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Themes/Shared/dailog/main.css" rel="stylesheet" type="text/css" />
    <link href="/Content/Themes/Shared/jqf1/main.css" rel="stylesheet" type="text/css" />
    <script src="/Scripts/jquery-1.7.1.min.js" type="text/javascript"></script>
    <script src="/Scripts/jquery-ui.min.js" type="text/javascript"></script>
    @*<script src="/Scripts/jquery-ui.js" type="text/javascript"></script>*@
    <script src="/Scripts/json.js" type="text/javascript"></script>
    <script src="/Scripts/Common.js" type="text/javascript"></script>
    <script src="/Scripts/jquery.alert.js" type="text/javascript"></script>
    <script src="/Scripts/jquery.ifrmdailog.js" type="text/javascript"></script>
    <script src="/Scripts/jquery.jqf1.js" type="text/javascript"></script>
    <script src="/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    @RenderSection("head", false)
    @RenderSection("JavaScript", false)
    
    @{
        if (System.Configuration.ConfigurationManager.AppSettings["IsRelease"] == "1" && Session["UserLevel"] != null && (short)Session["UserLevel"] >= 600)
        {
        <script>
            (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                    (i[r].q = i[r].q || []).push(arguments)
                }, i[r].l = 1 * new Date(); a = s.createElement(o),
                m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
            })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');
            ga('create', 'UA-85964254-1', 'auto');
            ga('send', 'pageview');
        </script>
        }
    }
</head>
<body>
    <div>@RenderBody()</div>
</body>
</html>
