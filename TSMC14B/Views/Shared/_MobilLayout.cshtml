    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="/Scripts/jquery-mobile/jquery.mobile.theme-1.4.5.min.css" rel="stylesheet" />
    @*<link href="/Scripts/jquery-mobile/jquery.mobile-1.4.5.min.css" rel="stylesheet" />*@
    <link href="http://demos.jquerymobile.com/1.4.5/css/themes/default/jquery.mobile-1.4.5.min.css" rel="stylesheet" />
    
    <link href="/Scripts/jquery-mobile/jquery.mobile.icons-1.4.5.min.css" rel="stylesheet" />
    <script src="/Scripts/jquery-1.7.1.min.js" type="text/javascript"></script>
    <script src="/Scripts/jquery-mobile/jquery.mobile-1.4.5.min.js"></script>

    @RenderSection("head", false)
    
    @{
        if (System.Configuration.ConfigurationManager.AppSettings["IsRelease"] == "1" && Session["UserLevel"] != null && (short)Session["UserLevel"] >= 600)
        {
        <script>
            (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                    (i[r].q = i[r].q || []).push(arguments)
                }, i[r].l = 1 * new Date(); a = s.createElement(o),
                m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
            })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');
            ga('create', 'UA-85964254-1', 'auto');
            ga('send', 'pageview');
        </script>
        }
    }

</head>
<body>
    @RenderBody()
</body>
</html>
